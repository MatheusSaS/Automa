{
  "name": "@automa/db",
  "private": true,
  "version": "0.1.0",
  "exports": {
    ".": "./index.ts",
    "./prisma": "./prisma.ts"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "db:generate": "pnpm with-env prisma generate --no-engine",
    "db:push": "pnpm with-env prisma db push",
    "studio": "pnpm with-env prisma studio --port 5556",
    "format": "prisma format && prettier --check \"**/*.{mjs,ts,json}\"",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "peerDependencies": {
    "typescript": "^5.5.4",
    "eslint": "^8.48.0",
    "prettier": "^3.2.4"
  },
  "dependencies": {
    "@prisma/client": "5.21.1",
    "@prisma/adapter-neon":"^5.21.1",
    "@neondatabase/serverless":"^0.10.1",
    "kysely": "^0.27.4",
    "prisma-extension-kysely": "^2.1.0"
  },
  "devDependencies": {
    "@automa/eslint-config": "workspace:*",
    "@automa/prettier-config": "workspace:*",
    "@automa/tsconfig": "workspace:*",
    "dotenv-cli": "^7.4.2",
    "prisma": "^5.21.1",
    "prisma-kysely": "^1.8.0"
  },
  "eslintConfig": {
    "extends": [
      "@automa/eslint-config/base"
    ],
    "rules": {
      "@typescript-eslint/consistent-type-definitions": "off"
    }
  },
  "prettier": "@automa/prettier-config"
}